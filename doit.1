#!/bin/bash

# This script takes a series and an address as arguments
# and opens a URL constructed from them.

# Check if two arguments are provided
if [ "$#" -gt 3 ]; then
    echo "Usage: $0 <series> <address>"
    exit 1
fi

SERIES=$1
ADDRESS=$2

# Construct the URL
URL="http://dalledress.io/dalle/${SERIES}/${ADDRESS}$3"

echo "Opening ${URL}"

# Use the 'open' command on macOS to open the URL in the default browser
open "${URL}"
